            %img(src="assets/img/team-4.jpg" alt="")/

    %section.main-contact
      .container
        .main-general__title.main-general__title--contact
          %p Contáctanos
        .main-contact__content
          .main-contact__box
            %form
              .row
                .col
                  %input.form-control{:placeholder => "Nombre ", :type => "text"}/
                .col
                  %input.form-control{:placeholder => "Teléfono", :type => "phone"}/
              .row
                .col
                  %input.form-control{:placeholder => "Email", :type => "email"}/
                .col
                  %input.form-control{:placeholder => "Servicio", :type => "text"}/

              %textarea#comment.form-control{:placeholder => "Mensaje",:rows => "5"}
              .main-cta__btn
                %a.btn_custom.btn--large.btn--filled-blue{ :href=>""}
                  Enviar
    %footer.main-footer
       
      .container
        .main-general__title.main-general__title--footer
          %p Ubicanos
        .main-footer__box
          .main-footer__boximg
            .main-footer__img 
              %img{ :src => "assets/img/footer.png"}/
          .main-footer__content
            .main-footer__item
              .main-footer__info
                %ul
                  %li
                    %a{:href=>"mailto:mtello@contactmarketing.cl"} mtello@contactmarketing.cl
                  %li
                    %a{:href=>"tel:56933985125"} +569 3398 5125
                  %li
                    %a{:href=>"#"} Avenida apoquindo 6410, oficina 605, Las Condes
                  
            .main-footer__item
              .main-footer__redes
                %a{ href: 'https://www.facebook.com/Contact-Marketing-904814839714398'  }
                  %img{ :src => "assets/img/facebook-letter-logo.png" }/
                %a{ href: '#' }
