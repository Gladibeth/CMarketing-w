                  .main-portfolio__item
                    .main-portfolio__img 
                      %img{ :src => "https://images.unsplash.com/photo-1517694712202-14dd9538aa97?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=750&q=80"}/ 
                  .main-portfolio__item
                    .main-portfolio__img 
                      %img{ :src => "https://images.unsplash.com/photo-1517694712202-14dd9538aa97?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=750&q=80"}/ 
                .main-cta__btn
                  %a.btn_custom.btn--large.btn--filled{ :href=>""}
                    Todos los proyectos
            #pills-profile.tab-pane.fade{"aria-labelledby" => "pills-profile-tab", :role => "tabpanel"} 
              .container
                .main-portfolio__grid
                  .main-portfolio__item
                    .main-portfolio__img 
                      %img{ :src => "https://images.unsplash.com/photo-1488190211105-8b0e65b80b4e?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=500&q=60"}/ 
                  .main-portfolio__item
                    .main-portfolio__img 
                      %img{ :src => "https://images.unsplash.com/photo-1488190211105-8b0e65b80b4e?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=500&q=60"}/ 
                  .main-portfolio__item
                    .main-portfolio__img 
                      %img{ :src => "https://images.unsplash.com/photo-1488190211105-8b0e65b80b4e?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=500&q=60"}/ 
                  .main-portfolio__item
                    .main-portfolio__img 
                      %img{ :src => "https://images.unsplash.com/photo-1488190211105-8b0e65b80b4e?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=500&q=60"}/
                .main-cta__btn
                  %a.btn_custom.btn--large.btn--filled{ :href=>""}
                    Todos los proyectos
            #pills-contact.tab-pane.fade{"aria-labelledby" => "pills-contact-tab", :role => "tabpanel"}
              .container
                .main-portfolio__grid
                  .main-portfolio__item
                    .main-portfolio__img 
                      %img{ :src => "https://images.unsplash.com/photo-1423666639041-f56000c27a9a?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=500&q=60"}/ 
                  .main-portfolio__item
                    .main-portfolio__img 
                      %img{ :src => "https://images.unsplash.com/photo-1423666639041-f56000c27a9a?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=500&q=60"}/ 
                  .main-portfolio__item
                    .main-portfolio__img 
                      %img{ :src => "https://images.unsplash.com/photo-1423666639041-f56000c27a9a?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=500&q=60"}/ 
                  .main-portfolio__item
                    .main-portfolio__img 
                      %img{ :src => "https://images.unsplash.com/photo-1423666639041-f56000c27a9a?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=500&q=60"}/
                .main-cta__btn
                  %a.btn_custom.btn--large.btn--filled{ :href=>""}
                    Todos los proyectos

    %section.main-equipo
      .main-general__title.main-general__title--team
